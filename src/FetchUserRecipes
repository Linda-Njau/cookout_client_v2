import { useEffect } from 'react';
import { fetchRecipes } from './httpService';

const FetchUserRecipes = ({ setRecipes, onSuccess }) => {
    useEffect(() => {
        const fetchUserRecipes = async () => {
            try {
                const user_id = 1;
                const recipesData = await fetchRecipes(user_id);
                setRecipes(recipesData)
            } catch (err) {
                console.error('Error fetching recips', err);
            }
        };
        fetchUserRecipes()
    }, [setRecipes, onSuccess]);
}

export default FetchUserRecipes;
